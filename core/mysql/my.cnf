[mysqld]
# Skip fsync on each commit - biggest speed boost for bulk imports
innodb_flush_log_at_trx_commit = 0

# Skip doublewrite buffer - not needed during import
innodb_doublewrite = OFF

# Larger buffer pool for caching
innodb_buffer_pool_size = 1G

# Larger redo log capacity reduces checkpoint stalls
innodb_redo_log_capacity = 512M
